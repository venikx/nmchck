{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "lib": ["ES2021"],
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "outDir": "dist",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
